#include "lists.h"
/**
 * pop_listint - deletes the head node and returns the head nodeâ€™s data (n)
 * @head: a pointer to the first node of the list
 *
 */
int pop_listint(listint_t **head)
{
	listint_t *t;
	int i;

	if (*head == NULL)
		return (NULL);
	t = *head;
	i = (*head)->n;
	*head = (*head)->next;
	free(t);

	return (i);
}
